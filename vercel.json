{
    "version": 2,
    "bulids": [
        {
           "src" : "movie_booking/wsgi.py",
           "use" : "@vercel/python",
           "config": { "maxLambdaSize": "15mb", "runtime":"python3.9" }
        },
        {
            "src" : "build_files.sh",
            "use" : "@vercel/static-build",
            "config": { 
                "distDir": "staticfiles-bulid"
             } 
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "movie_booking/wsgi.py"
        }
    ]
}